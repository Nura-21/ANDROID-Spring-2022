<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ToDO" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/material_dynamic_neutral20</item>
        <item name="colorPrimaryVariant">@color/material_dynamic_neutral20</item>
        <item name="colorOnPrimary">@color/material_dynamic_neutral20</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/material_dynamic_neutral20</item>
        <item name="colorSecondaryVariant">@color/material_dynamic_neutral20</item>
        <item name="colorOnSecondary">@color/material_dynamic_neutral20</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="ThemeOverlay.ToDO.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/material_dynamic_neutral20</item>
        <item name="fullscreenTextColor">@color/material_dynamic_neutral20</item>
    </style>
</resources>